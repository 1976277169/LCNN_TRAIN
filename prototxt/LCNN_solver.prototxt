net: "LCNN_train_test.prototxt"

test_iter: 2500
test_interval: 1000
test_compute_loss: true

base_lr: 0.01
#base_lr: 0.00005
momentum: 0.9
weight_decay: 0.00005
lr_policy: "inv"
#lr_policy: "fixed"
gamma: 0.000005
power: 0.75

display: 100
max_iter: 5000000
snapshot: 10000
snapshot_prefix: "snapshot/"

solver_mode: GPU

debug_info: false
